  android:layout_width="match_parent"
  android:layout_height="match_parent">
  <RelativeLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerInParent="true">
      <TextView android:id="@+id/textView2" android:layout_height="wrap_content" android:layout_below="@+id/tag_widget_name" android:layout_centerHorizontal="true" android:text="@string/no_maps_line2" android:layout_width="wrap_content" android:textSize="20sp"></TextView>
      <TextView android:id="@+id/tag_widget_name" android:text="@string/no_maps_line1" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="20sp"></TextView>
  </RelativeLayout>
</RelativeLayout>
